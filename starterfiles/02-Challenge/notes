inquirer.promt([
    {
        type: "list"
        message: "what shape do you want"
        name : "shape"
        choices: "circle" "square"
    }
    {
        type: "input"
        message: "what color would you like"
        name : "shapeColor"
    }
]).then (response => {
    if(response.shape == "circle){
        const shape = new circle(response.shapeColor)
    }
})