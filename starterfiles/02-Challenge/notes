class Svg {
    constructor(){
        thi.version = "1.1";
        this.width = "300";
        this.height = "200";
        this.xmlns = "http://www.w3.org/2000/srg"
    }
    render(){
        return `svg version=${this.version} width=${this.width} height=${this.height}`
    }
}

